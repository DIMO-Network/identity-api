# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Time
scalar Address

type Vehicle {
  id: ID!
  owner: Address!
  make: String!
  model: String!
  year: Int!
  mintTime: Time!
  totalCount: Int
  pageInfo: PageInfo
  edges: [Edge]
}

type PageInfo {
  endCursor: String!
  hasNextPage: Boolean
}

type Edge {
  node: Node
  cursor: String
}

type Node {
  id: String
}

type Query {
  ownedVehicles(address: Address!, first: Int, after: String): [Vehicle!]!
}
