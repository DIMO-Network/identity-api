# Sacd Types

type Sacd {
  """
  Recipient of sacd permission grant
  """
  grantee: Address!
  """
  Hex string of permissions
  """
  permissions: String!
  """
  Permission source
  """
  source: String!
  """
  The block timestamp at which this permission was set.
  """
  createdAt: Time!
  """
  The block timestamp at which the permission expires.
  """
  expiresAt: Time!
}

type Template {
    id: BigInt!
    creator: Address!
    asset: Address!
    permissions: String!
    cid: String!
    createdAt: Time!
}

type SacdEdge {
  node: Sacd!
  cursor: String!
}

"""
The Connection type for Sacds.
"""
type SacdConnection {
  totalCount: Int!
  edges: [SacdEdge!]!
  nodes: [Sacd!]!
  pageInfo: PageInfo!
}
